@model MedicalBilling.ViewModel.BillingViewModel

<table class="table">
    <thead>
        <tr>
            <th>SlNo</th>
            <th>Product</th>
            <th>Inventory ID</th>
            <th>Quantity</th>
            <th>Free</th>
        </tr>
    </thead>
    <tbody>
        @foreach (InvoiceItem item in Model.InvoiceItems)
        {
            <tr>
                <td>1</td>
                <td>@item.pid</td>
                <td>@item.iid</td>
                <td>@item.quantity</td>
                <td>@item.free</td>
            </tr>
        }
    </tbody>
</table>

