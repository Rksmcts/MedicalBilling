@model IEnumerable<MedicalBilling.Type>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdminPanel.cshtml";
}

<h2>Types</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-info" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.tid }, new { @class = "btn btn-warning btn-sm" })@* |
    @Html.ActionLink("Details", "Details", new { id=item.tid }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.tid }, new { @class = "btn btn-danger btn-sm" })
        </td>
    </tr>
}

</table>
